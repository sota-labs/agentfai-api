{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.hbs",
      "**/*.png"
    ]
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "src",
      "entryFile": "starters/api"
    },
    "agent-seeder": {
      "type": "application",
      "root": "src",
      "entryFile": "starters/agent/agent.seeder"
    },
    "buy-tester": {
      "type": "application",
      "root": "src",
      "entryFile": "starters/buy.tester"
    },
    "sell-tester": {
      "type": "application",
      "root": "src",
      "entryFile": "starters/sell.tester"
    }
  }
}