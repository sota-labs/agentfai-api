{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.hbs",
      "**/*.png"
    ]
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "src",
      "entryFile": "starters/api"
    },
    "agent-seeder": {
      "type": "application",
      "root": "src",
      "entryFile": "starters/agent/agent.seeder"
    },
    "buy": {
      "type": "application",
      "root": "src",
      "entryFile": "examples/order-buy.example"
    },
    "sell": {
      "type": "application",
      "root": "src",
      "entryFile": "examples/order-sell.example"
    }
  }
}